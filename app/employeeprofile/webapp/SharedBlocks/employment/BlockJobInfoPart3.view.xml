<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:layout="sap.ui.layout"
		xmlns="sap.m">


	<layout:HorizontalLayout class="sapUiSmallMarginTop" busy="{ReportInfoModel>/busy}"
					  busyIndicatorDelay="0" >
		<layout:VerticalLayout>
			<Label text="Manager"/>
			<layout:HorizontalLayout>
					<layout:content>
						<layout:VerticalLayout>
							<Text text="{ReportInfoModel>/Users/0/User/manager}"/>
							<Text text="{ReportInfoModel>/Users/0/User/salary}"/>

						</layout:VerticalLayout>
					</layout:content>
			</layout:HorizontalLayout>

		</layout:VerticalLayout>
	</layout:HorizontalLayout>


</mvc:View>