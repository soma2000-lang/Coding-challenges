<mvc:View
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:forms="sap.ui.layout.form"
		xmlns="sap.m">


	<forms:SimpleForm labelSpanL="4" labelSpanM="4"
					  labelSpanS="4" emptySpanL="0" emptySpanM="0" emptySpanS="0"
					  maxContainerCols="2" editable="false"
					  layout="ResponsiveGridLayout"
					  width="100%" busy="{ReportInfoModel>/busy}"
					  busyIndicatorDelay="0">
		<Label text="Start Date"/>
		<Text text="{ReportInfoModel>/Users/0/EmpJob/startDate}"/>

		<Label text="End Date"/>
		<Text text="{ReportInfoModel>/Users/0/EmpJob/endDate}"/>

		
	</forms:SimpleForm>


</mvc:View>